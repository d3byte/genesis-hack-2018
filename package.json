{
  "name": "survey-widget",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "build:component": "ng build --prod --output-hashing none && node concatenate.js",
    "swagger-generate": "./swagger-generate.sh",
    "format": "prettier --list-different \"src/**/*.{ts,js,json,scss}\"",
    "commit": "git cz",
    "lint": "yarn run lint:check && yarn run lint:style && yarn run format:check",
    "lint:check": "eslint \"src/**/*.ts\" && tsc --noemit",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "style:fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "format:check": "prettier --check src/**/*.ts",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json,scss}\"",
    "build:ci": "ng build --aot=true --configuration=production --optimization=false --sourceMap=false"
  },
  "husky": {
    "hooks": {
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --syntax scss --fix",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/elements": "^9.0.4",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@ngrx/store": "1.2.1",
    "angular-svg-icon": "^9.0.0",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/qs": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "codelyzer": "^5.0.0",
    "concat": "^1.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "fs-extra": "^8.1.0",
    "git-describe": "^4.0.4",
    "htmllint": "^0.8.0",
    "htmllint-cli": "^0.0.7",
    "husky": "^4.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.10.2",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.12.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "typescript-formatter": "^7.2.2"
  }
}
